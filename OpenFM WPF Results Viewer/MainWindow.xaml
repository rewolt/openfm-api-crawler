<Window x:Class="OpenFM_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenFM_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="531.499" Width="426.98">
    <Grid>
        <Button x:Name="bt_refresh" Content="Refresh" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="auto"/>
        <Button x:Name="bt_plus" Content="+" HorizontalAlignment="Right" Margin="0,10,30,0" VerticalAlignment="Top" Width="15"/>
        <Button x:Name="bt_minus" Content="-" HorizontalAlignment="Right" Margin="0,10,10,0" VerticalAlignment="Top" Width="15"/>
        <TreeView ItemsSource="{Binding Channels}" x:Name="tvChannels" HorizontalAlignment="Stretch" Margin="10,35,10,10" VerticalAlignment="Stretch"/>

    </Grid>
</Window>
